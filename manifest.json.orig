{
  "name": "Add Tabs to Bookmark Folder",
  "version": "1.0",
  "description": "A Chrome extension that adds all open tabs to a bookmark folder.",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "bookmarks"
  ]
}
